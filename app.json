{
  "expo": {
    "name": "Jal-Chetna",
    "slug": "gis-dashboard",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/adaptive-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app uses your location to find nearby groundwater stations."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "${GOOGLE_MAPS_API_KEY}"
        }
      },
      "package": "com.devrohitkumar.gisdashboard"
    },
    "web": {
      "favicon": "./assets/adaptive-icon.png"
    },
    "plugins": ["expo-secure-store", "expo-localization", "expo-location"],
    "extra": {
      "eas": {
        "projectId": "bc6c8abe-bbc5-43cf-b663-0cbecce583c8"
      }
    }
  }
}
